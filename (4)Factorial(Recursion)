#FACTORIAL
def factorial(num):
    
    if num <= 1:
        return 1
    else:
        result = num * factorial(num-1)
        return result
print(factorial(4))
#######################################################

def fac(num):
    if num <= 1:
        return 1
    else:
        result = num * fac(num-1)
        return result
num = 4
temp = fac(num)
#print(temp)
txt = "Factorial of {} = {}".format(num, temp)
print(txt)


